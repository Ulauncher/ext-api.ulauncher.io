{
    "search.exclude": {
        ".mypy_cache": true,
        ".pytest_cache": true
    },
    "files.exclude": {
        ".cache": true,
        ".db": true,
        ".mypy_cache": true,
        ".pytest_cache": true
    },
    "python.analysis.typeCheckingMode": "strict",
    "mypy-type-checker.ignorePatterns": ["**/*.py"],
    "python.testing.pytestArgs": ["tests"],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true
}
